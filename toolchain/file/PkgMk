# Description: File type guesser
# URL: http://www.darwinsys.com/file/
# Maintainer: Remi Pommarel, repk at triplefau dot lt


NAME=file
VERSION=5.14
HOST_PKG=1


DEPENDS=""
SOURCES="ftp://ftp.astron.com/pub/${NAME}/${NAME}-${VERSION}.tar.gz"


pkgmain() {
	cd ${NAME}-${VERSION}

	./configure --prefix=${PKG}

	make -j${NR_JOBS}
	make install
}
